{
  "manifest_version": 2,
  "name": "jsblocks",
  "description": "block stuff you don't want to see",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "main.html"
  },
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ]
}